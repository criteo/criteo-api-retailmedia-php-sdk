        "criteo",
        "openapi generator",
        "php",
        "sdk",
        "rest",
        "api"
    ],
    "homepage": "https://api.criteo.com",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Criteo",
            "homepage": "https://github.com/criteo/criteo-api-retailmedia-php-sdk"
        }
