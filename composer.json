{
    "name": "criteo/criteo-api-retailmedia-sdk",
    "description": "PHP SDK for Criteo API 0.0.240207 version",
    "keywords": [
        "criteo",
        "openapi generator",
        "php",
        "sdk",
        "rest",
        "api"
    ],
    "homepage": "https://api.criteo.com",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Criteo",
            "homepage": "https://github.com/criteo/criteo-api-retailmedia-php-sdk"
        }
    ],
    "scripts": {
        "test": "phpunit test/Api"
    },
    "require": {
        "php": "^7.4 || ^8.0",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.3",
        "guzzlehttp/psr7": "^1.7 || ^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": ">=7.4 <10.0",
        "squizlabs/php_codesniffer": "~2.6",
        "friendsofphp/php-cs-fixer": "~2.12",
        "mockery/mockery": "^1.3",
        "jchook/phpunit-assert-throws": "^1.0"
    },
    "autoload": {
        "psr-4": { "criteo\\api\\retailmedia\\preview\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "criteo\\api\\retailmedia\\preview\\" : "test/" }
    }
}
